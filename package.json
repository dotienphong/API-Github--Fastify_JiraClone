{
  "name": "fastify-api",
  "version": "1.0.0",
  "description": "This project with Fastify.",
  "main": "server.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "node --test test/**/*.test.js",
    "format": "prettier --write \"./**/*.{js,jsx,json}\"",
    "start": "fastify start -l info ./src/server.js --env-file .env",
    "dev": "bun run format && bun run --watch ./src/server.js --env-file .env",
    "build": "bun build ./src/server.js --target=bun --outfile=./dist/server.js"
  },
  "keywords": [],
  "author": "Do Tien Phong",
  "license": "ISC",
  "dependencies": {
    "@fastify/cookie": "^10.0.0",
    "@fastify/cors": "^10.0.0",
    "@fastify/env": "^5.0.0",
    "@fastify/rate-limit": "^10.0.0",
    "@fastify/swagger": "^9.0.0",
    "@fastify/swagger-ui": "^5.0.1",
    "axios": "^1.7.7",
    "bcrypt": "^5.1.1",
    "dotenv": "^16.4.5",
    "escape-html": "^1.0.3",
    "fastify": "^5.0.0",
    "fs": "^0.0.1-security",
    "http": "^0.0.1-security",
    "jsonwebtoken": "^9.0.2",
    "nodemon": "^3.1.7",
    "pg": "^8.13.0",
    "pino-pretty": "^11.2.2",
    "prettier": "^3.3.3",
    "redis": "^4.7.0",
    "uuid": "^10.0.0",
    "winston": "^3.14.2"
  }
}
